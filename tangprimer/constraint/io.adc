set_pin_assignment {clk} { LOCATION = K14; }		##24MHZ
set_pin_assignment {rst_n} { LOCATION = K16; }		##USER_KEY

## RGB LEDs, 3 pins
set_pin_assignment {rgb_led[0]} { LOCATION = R3;  }	##LED_R, R3
set_pin_assignment {rgb_led[1]} { LOCATION = J14;  }	##LED_B, J14
set_pin_assignment {rgb_led[2]} { LOCATION = P13;  }	##LED_G, P13

## UART0 (GPIO 16,17)
set_pin_assignment {uart0_rx} { LOCATION = J13; }
set_pin_assignment {uart0_tx} { LOCATION = H13; }